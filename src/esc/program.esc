func2 :: {
    cond : bool : <>
    if false {
        return 0
    }
    if !cond { return 100 }
    return 9999 // unreachable
}
func1 :: {
    a := 2
    b := a + func2 false
    return b
}
a := 13
a = 39
b := a + func1 + 5
b = b + (-1)
print a
print b
format :: "C"
print format.Length
print (b.ToString format) // C=currency
return // early return
